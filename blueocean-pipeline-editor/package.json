    "eslint-to-editorconfig": "1.2.0",
    "gulp": "3.9.1",
    "gulp-mocha": "2.2.0",
    "gulp-babel": "6.1.2",
    "gulp-copy": "0.0.2",
    "gulp-eslint": "3.0.1",
