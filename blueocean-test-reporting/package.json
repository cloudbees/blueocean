{
  "name": "blueocean-test-reporting",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "git@github.com:jenkinsci/blueocean-plugin",
  "scripts": {
    "storybook": "start-storybook -p 9001 -s node_modules/@jenkins-cd/design-language/dist/assets/,target/classes/org/jenkins/ui/jsmodules/blueocean_personalization/",
    "lint": "jjsbuilder --tasks lint",
    "lint:fix": "jjsbuilder --tasks lint --fixLint",
    "lint:watch": "jjsbuilder --tasks lint:watch --continueOnLint",
    "test": "jjsbuilder --tasks test",
    "test:watch": "jjsbuilder --tasks test:watch",
    "bundle": "tsc && tse src/main/ts src/main/js && jjsbuilder --tasks bundle",
    "bundle:watch": "jjsbuilder --tasks bundle:watch",
    "mvnbuild": "tsc && tse src/main/ts src/main/js && jjsbuilder --tasks bundle",
    "mvntest": "jjsbuilder --tasks test,lint",
    "tse": "tse src/main/ts src/main/js",
    "jjs": "jjsbuilder --tasks bundle"
  },
  "devDependencies": {
    "@jenkins-cd/design-language": "0.0.86",
    "@jenkins-cd/eslint-config-jenkins": "0.0.2",
    "@jenkins-cd/js-builder": "0.0.47",
    "@kadira/storybook": "2.20.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.46",
    "@types/react": "^0.14.44",
    "babel": "6.5.2",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "blueocean-js-extensions-typescript-support": "0.0.1",
    "chai": "3.5.0",
    "enzyme": "2.4.1",
    "eslint": "2.13.1",
    "eslint-plugin-react": "4.3.0",
    "eslint-to-editorconfig": "1.2.0",
    "gulp": "3.9.1",
    "gulp-mocha": "3.0.1",
    "mocha": "3.1.0",
    "moment": "^2.15.2",
    "nock": "8.0.0",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "^3.0.0",
    "typescript": "^2.0.6"
  },
  "dependencies": {
    "@jenkins-cd/js-extensions": "0.0.27",
    "@jenkins-cd/js-modules": "0.0.8",
    "blueocean-js-extensions": "0.0.1",
    "react": "15.3.2"
  },
  "jenkinscd": {
    "import": [
      "blueocean-js-extensions",
      "react"
    ]
  }
}
